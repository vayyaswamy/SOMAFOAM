FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      electron;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 5E14;

boundaryField
{
    electrode
    {
        type            driftDiffusionElectronDensity;
	seec		0.05;
	Edepend		true;
	field_emission	false;
	field_enhancement_factor	55.0;
	work_function	5.0; // in eV
    }
    ground
    {
        type            driftDiffusionElectronDensity;
	seec		0.05;
	Edepend		true;
	field_emission	false;
	field_enhancement_factor	55.0;
	work_function	5.0 ; // in eV
    }
    otherfaces
    {
        type            empty;
    }
}



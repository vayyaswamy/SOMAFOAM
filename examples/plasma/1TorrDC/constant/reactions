FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      reactions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

species
(
	electron
	Arp1
	Arm
	Ar
);

reactions
(

        electronImpactElasticTabularReaction
        electron + Ar = electron + Ar
        ("$FOAM_CASE/constant/reaction_1")

        electronImpactInelasticTabularReaction
		electron + Ar = electron + electron + Arp1
        ("$FOAM_CASE/constant/reaction_2")
        (2.5248E-18)

        electronImpactInelasticTabularReaction
		electron + Ar = electron + Arm
        ("$FOAM_CASE/constant/reaction_3")
        (1.851912e-18)

        electronImpactInelasticTabularReaction
		electron + Arm = electron + electron + Arp1
        ("$FOAM_CASE/constant/reaction_met")
        (7.094438e-19)	

        electronImpactInelasticArrheniusReaction
		Arm + electron = Ar + electron
        (9.996686e11 0 0)
        (-1.851912e-18)	

        irreversibleArrheniusReaction
		Arm + Arm = Arp1 + Ar + electron
        (1.694853121090E11 0 0)

        irreversibleArrheniusReaction
		Arm + Ar = Ar + Ar
        (1.80663e6 0 0)
);

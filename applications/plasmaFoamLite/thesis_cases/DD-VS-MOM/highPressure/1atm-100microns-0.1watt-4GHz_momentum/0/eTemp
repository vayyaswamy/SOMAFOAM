FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      eTemp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 44000;

boundaryField
{
    ground
    {
        type            zeroGradient;//electronTemperature;
        seec            0.0;
        Tse             0;
        value           uniform 1e-30;
	Edepend		1;
    }
    electrode
    {
        type            zeroGradient;//electronTemperature;
        seec            0.0;
        Tse             0;
        value           uniform 1e-30;
	Edepend		1;

    }
}



FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      eden;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -2 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    ground
    {
        type            zeroGradient;
    }
    electrode
    {
        type            zeroGradient;
    }
}



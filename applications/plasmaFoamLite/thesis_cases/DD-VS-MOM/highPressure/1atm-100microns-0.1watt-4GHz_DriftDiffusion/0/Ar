FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ar;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 2.5e25;// 6.5789474e+22;

boundaryField
{
    ground
    {
        type            zeroGradient;
    }
    electrode
    {
        type            zeroGradient;
    }
}



FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      electron;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 0.5e21;

boundaryField
{   
    ground
    {
        type            driftDiffusionElectronDensity;//zeroGradient;// momentumPositiveIonDensity;
        seec            0.0;
	Edepend         true;

    }
    electrode
    {
        type            driftDiffusionElectronDensity;//zeroGradient;// momentumPositiveIonDensity;
	seec		0.0;
	Edepend         true;

    }
}



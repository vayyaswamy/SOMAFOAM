FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Arp1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 0.5e21;

boundaryField
{
    ground
    {
        type         zeroGradient;//noInletFlux;// momentumPositiveIonDensity;//zeroGradient;//driftDiffusionPositiveIonDensity;
	vTherm	     0;
	seec	     0;
    }

    electrode
    {
        type         zeroGradient;//noInletFlux;// momentumPositiveIonDensity;//zeroGradient;//driftDiffusionPositiveIonDensity;
	vTherm	     0;
	seec	     0;
    }
    otherElectrodes
    {
        type            zeroGradient;
    }

    intoThePaper
    {
        type            empty;
    }
}


